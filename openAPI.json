    "/login": {
      "post": {
        "summary": "Login All Users",
        "description": "Login for All Users",
        "tags": [
          "Users"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "member@gmail.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "member123"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Berhasil"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "integer",
                          "example": 2
                        },
                        "full_name": {
                          "type": "string",
                          "example": "member"
                        },
                        "email": {
                          "type": "string",
                          "example": "member@gmail.com"
                        },
                        "pasword": {
                          "type": "string",
                          "example": "xxxx"
                        },
                        "role": {
                          "type": "string",
                          "example": "member"
                        },
                        "createdAt": {
                          "type": "string",
                          "example": "2022-10-20T18:04:13.907Z"
                        },
                        "updatedAt": {
                          "type": "string",
                          "example": "2022-10-20T18:04:13.907Z"
                        },
                        "token": {
                          "type": "string",
                          "example": "xxxxx"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "User login fail",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "FAIL"
                    },
                    "message": {
                      "type": "string",
                      "example": "Eror message"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "User login fail",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "FAIL"
                    },
                    "message": {
                      "type": "string",
                      "example": "email atau password salah"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
